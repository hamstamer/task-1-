   "source": [
    "import cv2\n",
    "img=cv2.imread(\"wordykiddie.png\")\n",
    "\n",
    "y=cv2.add(img,100)\n",
    "cv2.imshow(\"Hbright\",y)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "\n",
    "z=cv2.subtract(img,100)\n",
    "cv2.imshow(\"Lbright\",z)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
